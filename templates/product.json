        "custom_liquid_yAhEKK": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<!-- Trigger Button in the content section -->\n<button class=\"trigger\">Size Guide<\/button>\n\n\n\n\n<style>\n\/* Button styling *\/\n.trigger {\n    color: black;\n    cursor: pointer;\n    text-decoration: underline;\n    background: none;\n    border: none;\n    font-size: 17px;\n    padding: 0;\n}\n\n\/* Modal container *\/\n.modal {\n    position: fixed; \/* Sticks to the viewport *\/\n    left: 0;\n    top: 0;\n    width: 100%;\n    height: 100%;\n    background-color: rgba(0, 0, 0, 0.5); \/* Dark transparent background *\/\n    display: none; \/* Hidden by default *\/\n    align-items: center;\n    justify-content: center;\n    z-index: 999; \/* Ensures modal is on top *\/\n}\n\n\/* Modal content box *\/\n.modal-content {\n    background-color: white;\n   width: 600px; \/* Width of the modal *\/\n    text-align: center;\n    position: relative; \/* To position close button inside *\/\n}\n\n\/* Close button styling *\/\n.close-button {\n    position: absolute;\n    top: 0;\n    right: 0;\n    font-size: 30px;\n    font-weight: 100 !important;\n    cursor: pointer;\n    color: #000;\n    background: #f7d945;\n    padding: 10px;\n}\n\n\/* Show modal when active *\/\n.modal.show {\n    display: flex; \/* Flex to center the modal *\/\n}\n<\/style>\n\n<!-- Include jQuery if not already included -->\n<script src=\"https:\/\/code.jquery.com\/jquery-3.6.0.min.js\"><\/script>\n\n<script>\n$(document).ready(function() {\n    \/\/ When the trigger button is clicked, show the modal\n    $(\".trigger\").click(function() {\n        $(\".modal\").addClass(\"show\");\n    });\n\n    \/\/ Close the modal when the close button is clicked\n    $(\".close-button\").click(function() {\n        $(\".modal\").removeClass(\"show\");\n    });\n\n    \/\/ Close the modal when clicking outside of the modal content\n    $(window).click(function(event) {\n        if ($(event.target).hasClass(\"modal\")) {\n            $(\".modal\").removeClass(\"show\");\n        }\n    });\n});\n<\/script>"
          }
        },
