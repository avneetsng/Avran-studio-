    "custom_liquid_xKHmF4": {
      "type": "custom-liquid",
      "custom_css": [
        ".sub-head {margin-bottom: 40px; padding: 0; flex-wrap: wrap; column-gap: 1rem; row-gap: 1rem;}"
      ],
      "settings": {
        "custom_liquid": "{% assign subcategories = \"All, Everyday Classics, Festive Finesse, Timeless Solids\" | split: \",\" %}\n<div class=\"container subcategories\">\n  <ul class=\"sub-head\">\n    <!-- Manually set the URLs for each subcategory -->\n    <li><a href=\"\/collections\/all?subcategory=All\">All<\/a><\/li>\n    <li><a href=\"\/collections\/everyday-classics?subcategory=Everyday Classics\">Everyday Classics<\/a><\/li>\n    <li><a href=\"\/collections\/festive-finesse?subcategory=Festive Finesse\">Festive Finesse<\/a><\/li>\n    <li><a href=\"\/collections\/timeless-solids?subcategory=Timeless Solids\">Timeless Solids<\/a><\/li>\n  <\/ul>\n<\/div>\n\n<script>\n  document.addEventListener(\"DOMContentLoaded\", function() {\n    const urlParams = new URLSearchParams(window.location.search);\n    const selectedSubcategory = urlParams.get('subcategory');\n    \n    if (selectedSubcategory) {\n      const subcategoryElements = document.querySelectorAll('.subcategories ul li');\n      \n      subcategoryElements.forEach(function(subcategoryElement) {\n        if (subcategoryElement.textContent.trim() === selectedSubcategory) {\n          subcategoryElement.classList.add('active'); \/\/ Add the 'active' class\n        }\n      });\n    }\n  });\n<\/script>\n\n<style>\n.sub-head {\n    display: flex;\n    justify-content: start;\n    gap: 20px;\n}\n\n.sub-head li {\n    list-style: none;\n    padding: 5px 13px;\n    border-radius: 6px;\nborder:2px solid #000B3E;\n}\n\n.sub-head li.active {\n        background-color: #000B3E;\n    color: white;\n    padding: 5px 13px;\n    border-radius: 6px;\n}\n\n<\/style>"
      }
    },
